{
	"name": "wikihack-starter",
	"version": "0.1.0",
	"type": "module",
	"scripts": {
		"prebuild": "curl \"https://diff.wikimedia.org/feed/\" -o data/blog.rss",
		"start": "npm run prebuild && vite",
		"build": "npm run prebuild && vite build && cp -r data dist/data && cp _redirects dist/",
		"preview": "vite preview"
	},
	"dependencies": {
		"vue": "^3.4",
		"vue-router": "^4.3",
		"@wikimedia/codex": "^2.3.3",
		"@wikimedia/codex-icons": "^2.3.3",
		"@wikimedia/codex-design-tokens": "^2.3.3"
	},
	"devDependencies": {
		"vite": "^5.2",
		"@vitejs/plugin-vue": "^5.0",
		"less": "^4.2"
	}
}
